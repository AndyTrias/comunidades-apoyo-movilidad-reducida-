{{#partial "contenido"}}

    <main class="flex flex-col h-full">
        <div class="w-1/2 flex flex-col justify-center items-center p-10 container mx-auto my-4 flex flex-row" style="background-color: rgba(255, 255, 255, 0.5);">
            <h1 class="text-2xl mb-4">{{criterio}}</h1>
            <div class="flex flex-col items-center">

                {{#each entidades}}
                    <div class="flex flex-row justify-between w-full">
                        {{this.nombre}}
                    </div>
                {{/each}}

            </div>
        </div>
    </main>

{{/partial}}
{{> templates/templateBase }}