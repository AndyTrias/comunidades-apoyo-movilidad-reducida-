{{#partial "contenido"}}
    <div class="bg-white shadow-md rounded px-8 pt-6 pb-8 mb-4">
        <h2 class="text-2xl font-semibold mb-4 text-gray-800">Listado de Establecimientos</h2>
        <ul>
            {{#each establecimientos}}
                <li class="mb-6 border-b border-gray-300 pb-4">
                    <div class="mb-2">
                        <h3 class="text-lg font-semibold text-indigo-700">{{this.nombre}}</h3>
<!--                        <p class="text-gray-600">Localizaci√≥n: {{this.localizacion}}</p>-->
                    </div>
                    <div>
                        <p class="text-indigo-900 font-semibold mb-2">Servicios Prestados:</p>
                        <ul class="list-disc ml-6">
                            {{#each this.servicios}}
                                <li class="text-gray-700">{{this.nombre}}</li>
                                <!-- You can display more information about each servicio here if needed -->
                            {{/each}}
                        </ul>
                    </div>
                </li>
            {{/each}}
        </ul>
    </div>
{{/partial}}

{{> templates/template }}
